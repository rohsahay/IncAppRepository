<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping package="dto">  
  <class name="dto.LoginDto" table="USERLOGIN">
  	<meta attribute="class-description">
         This class contains user and admin login details. 
    </meta>  
    <id name="name" type="string" column="USERNAME">  
     <generator class="assigned"></generator>  
    </id>         
    <property name="password" type="string" column="PASSWORD"></property>
    <property name="privilege" type="string" column="PRIVILEGE"></property>               
  </class>             
 </hibernate-mapping>  